#include<iostream>
using namespace std;
void selectionsort(int arr,int size);
void swap(int *a,int *b);
   //Declaring the function
   void selectionsort(int arr)
   {
       int i,int j;
       for(i=0;i<5;i++)
       {
           for(j=i;j<5;j++)
           {
               if(arr[i]<arr[j])
               swap(&arr[i],&arr[j]);
           }
       }
   }
   void swap(int *a,int *b)
   {
       int temp;
       *a=temp;
       *a=*b;
       *b=temp;   
   }
   int main()
   {
       int i,
       int arr[5];
       for(i=0;i<5;i++)
       cout<<"The unsorted elements are\n";
       cin>>arr[i];
       selectionsort(int arr)
       cout<<"Array after sorting";
   }
